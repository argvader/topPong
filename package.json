{
  "name": "TopPong",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.js",
  "author": "argvader",
  "description": "THE ping pong leaderboard",
  "repository": {
    "type": "git",
    "url": "git@github.com:argvader/topPong.git"
  },
  "scripts": {
    "test": "node tools/test.js --env=jsdom",
    "coverage": "node tools/test.js --env=jsdom --coverage",
    "start": "$(npm bin)/webpack-dashboard -t 'TopPong' -- $(npm bin)/webpack-dev-server",
    "lint": "$(npm bin)/eslint ./src webpack.config.js -f table --ext .js --ext .jsx"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/__mocks__/*",
      "!src/**/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 38,
        "functions": 47,
        "lines": 55,
        "statements": 54
      }
    },
    "roots": [
      "<rootDir>/src"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/tools/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/tools/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "devDependencies": {
    "autoprefixer": "~6.7.2",
    "babel-core": "~6.22.1",
    "babel-eslint": "~7.1.1",
    "babel-loader": "~6.2.10",
    "babel-plugin-syntax-decorators": "~6.13.0",
    "babel-plugin-transform-class-properties": "~6.22.0",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "~6.22.0",
    "babel-plugin-transform-runtime": "~6.22.0",
    "babel-preset-es2015": "~6.22.0",
    "babel-preset-react": "~6.22.0",
    "babel-preset-react-hmre": "~1.1.1",
    "babel-preset-stage-0": "~6.22.0",
    "babel-runtime": "~6.22.0",
    "copy-webpack-plugin": "~4.0.1",
    "css-loader": "~0.26.1",
    "csswring": "~5.1.1",
    "dotenv": "~4.0.0",
    "enzyme": "~2.7.1",
    "eslint": "~3.15.0",
    "eslint-config-airbnb": "~14.0.0",
    "eslint-friendly-formatter": "~2.0.7",
    "eslint-loader": "~1.6.1",
    "eslint-plugin-class-property": "~1.0.3",
    "eslint-plugin-flowtype": "~2.30.2",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-jsx-a11y": "~3.0.2",
    "eslint-plugin-react": "~6.9.0",
    "extract-text-webpack-plugin": "~2.1.0",
    "file-loader": "~0.10.0",
    "html-webpack-plugin": "~2.28.0",
    "image-webpack-loader": "~3.2.0",
    "jest": "~19.0.2",
    "node-sass": "~4.5.0",
    "postcss-loader": "~1.2.2",
    "react-addons-test-utils": "~15.4.2",
    "react-hot-loader": "~1.3.1",
    "react-redux": "^5.0.3",
    "require-dir": "~0.3.1",
    "sass-loader": "~4.1.1",
    "shared-worker-loader": "git://github.com/jefftougas/shared-worker-loader.git#v0.2.0",
    "style-loader": "~0.13.1",
    "url-loader": "~0.5.7",
    "webpack": "~2.2.1",
    "webpack-combine-loaders": "~2.0.3",
    "webpack-dashboard": "~0.3.0",
    "webpack-dev-server": "~1.16.3"
  },
  "dependencies": {
    "immutable": "3.8.1",
    "react": "15.4.2",
    "react-addons-css-transition-group": "15.4.2",
    "react-cookie": "1.0.4",
    "react-dom": "15.4.2",
    "react-redux": "5.0.3",
    "react-router": "3.0.0",
    "react-tap-event-plugin": "2.0.1",
    "react-timeout": "1.0.0"
  }
}
